   ],
   "source": [
    "import numpy as np\n",
    "import scipy.stats as stat\n",
    "def pdf(x,mean,median):\n",
    "    data = stat.norm(mean,median)\n",
    "    pdf = data.pdf(x)\n",
    "    \n",
    "    return pdf\n",
    "pdf(4,12,34)"
