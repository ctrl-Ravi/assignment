     "execution_count": 25,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "from numpy.random import poisson\n",
    "import scipy.stats as stat\n",
    "def cdf(x,mean):\n",
    "    data= stat.poisson(mean)\n",
    "    cdf = data.cdf(x)\n",
    "    return \"cdf=\",cdf\n",
    "cdf(2,7)"
